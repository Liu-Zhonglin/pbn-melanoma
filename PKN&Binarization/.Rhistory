data <- read_csv("https://raw.githubusercontent.com/vincentarelbundock/Rdatasets/master/csv/Ecdat/Wages1.csv")  # Replace with the actual dataset link
# a) Generate a summary of the data
summary(data)
# b) Create an indicator variable for gender
data <- data %>% mutate(female_ind = ifelse(sex == "female", 1, 0))
# c) Find the mean of "female_ind"
mean_female_ind <- mean(data$female_ind)
mean_female_ind
# e) Find the correlation between "wage" and "school"
correlation <- cor(data$wage, data$school)
correlation
# g) Generate a scatterplot with a fitted line
ggplot(data, aes(x = school, y = wage)) +
geom_point() +
geom_smooth(method = "lm", se = FALSE) +
labs(x = "Years of Schooling", y = "Wage")
# h) Generate a box and whisker plot for wage by gender
ggplot(data, aes(x = sex, y = wage)) +
geom_boxplot() +
labs(x = "Gender", y = "Wage")
# g) Generate a scatterplot with a fitted line
